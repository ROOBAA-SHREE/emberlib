<div class="newuserbutton">
  {{#link-to "new-user" class="button"}}
  New User
  {{/link-to}}
</div>
<table class="with-detail">
  <tr>
    <td class="table-overview">
      <table class="user-table">
        <th>userid</th>
        <th>name</th>
        <th>email</th>
        {{#each this.userData.items as |item|}}
        <tr>
          <td>{{item.description.[0].userid}} </td>
          <td>{{#link-to "details"}}{{item.description.[0].name}} {{/link-to}}</td>
          <td>{{item.description.[0].email}} </td>
         <td> {{#link-to "edit"}}
          <button {{action "edituser" item.description}}>
            Edit
          </button>
          {{/link-to}}
          </td>
        </tr>
        {{/each}}
      </table>
    </td>
  </tr>
</table>

{{outlet}}